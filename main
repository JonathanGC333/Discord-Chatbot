    def ttt_winner(spot):
        global winner
        if ttt_mode == 'player':
            if spot == 'X':
                winner = player1
            if spot == 'O':
                winner = player2
        if ttt_mode == 'kidney':
            if spot == 'X':
                winner = "Kidney"
            if spot == 'O':
                winner = player
        return

    def ttt_win():
        if ttt1 == ttt2 == ttt3:
            if ttt1 != '+':
                ttt_winner(ttt1)
        if ttt4 == ttt5 == ttt6:
            if ttt4 != '+':
                ttt_winner(ttt4)
        if ttt7 == ttt8 == ttt9:
            if ttt7 != '+':
                ttt_winner(ttt7)
        if ttt1 == ttt4 == ttt7:
            if ttt1 != '+':
                ttt_winner(ttt1)
        if ttt2 == ttt5 == ttt8:
            if ttt2 != '+':
                ttt_winner(ttt2)
        if ttt3 == ttt6 == ttt9:
            if ttt3 != '+':
                ttt_winner(ttt3)
        if ttt1 == ttt5 == ttt9:
            if ttt1 != '+':
                ttt_winner(ttt1)
        if ttt3 == ttt5 == ttt7:
            if ttt3 != '+':
                ttt_winner(ttt3)

